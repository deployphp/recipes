    "type": "library",
    "keywords": [
        "deployer",
        "recipes",
        "deploy",
        "deployment",
        "cachetool",
        "cloudflare",
        "newrelic",
        "slack",
        "hipchat",
        "rabbit",
        "sentry",
        "yarn"
    ],
    "homepage": "https://github.com/deployphp/recipes",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Medvedev",
            "email": "anton@medv.io"
        }
    ],
    "support": {
        "issues": "https://github.com/deployphp/recipes/issues",
        "source": "https://github.com/deployphp/recipes"
